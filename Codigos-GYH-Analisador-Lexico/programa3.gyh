#Programa nao tem erro lexico

[DECLARAR]
parametro:INTEGER
fatorial:INTEGER

[PROGRAMA]
# Calcula o fatorial de um n√∫mero inteiro
LER parametro
fatorial := parametro 
SE parametro == 0 ENTAO fatorial := 1
ENQTO parametro <> 1
INICIO
fatorial := fatorial * (parametro - 1) 
parametro := parametro - 1
FINAL
IMPRIMIR fatorial
IMPRIMIR "Oi tudo bem? &&&&"
