#Erro Léxico: <_> caractere desconhecido

[DECLARAR]
parametro:INTEGER
fatorial:INTEGER

[PROGRAMA] 
# Calcula o fatorial de um número inteiro
LER parametro
fatorial := parametro 
SE parametro == 0 ENTAO fatorial := 1
ENQTO parametro > 1
INICIO
fatorial := fatorial * (parametro _ 1) 
parametro := parametro - 1
FINAL
IMPRIMIR fatorial
IMPRIMIR "Oi"
